{
  "name": "smashmate",
  "version": "4.0.0",
  "description": "A web application for Smash Ultimate nerds interested in seeing the raw data behind the Fighters.",
  "scripts": {
    "start": "npm run --prefix server start",
    "test": "npm run --prefix client test",
    "build": "npm-run-all --parallel build:*",
    "build:client": "webpack --mode=production",
    "build:server": "npm run --prefix server tsc",
    "dev": "npm-run-all --parallel dev:*",
    "dev:client": "webpack serve --mode=development",
    "dev:server": "npm run --prefix server dev:server",
    "dev:tsc:watch": "npm run --prefix server tsc:watch",
    "postinstall": "npm-run-all --parallel install:*",
    "install:client": "npm --prefix client ci",
    "install:server": "npm --prefix server ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DreSean-Sutton/smashmate.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DreSean-Sutton/smashmate/issues"
  },
  "homepage": "https://github.com/DreSean-Sutton/smashmate#readme",
  "dependencies": {
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.3",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "devDependencies": {
    "@types/jest": "^29.1.0",
    "npm-run-all": "^4.1.5",
    "webpack-dev-server": "^4.11.1"
  },
  "engines": {
    "npm": "8.x",
    "node": "18.x"
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
